@import url(http://fonts.googleapis.com/css?family=Raleway);

#realtor {
    font-style: oblique;
    font-weight: bolder;
    background-color: #fff;
    color: #00bf8f;
}

.RealtorListing {
    position: relative;
    top:63px;
    padding-left: 12%;
    margin-right: 8%;
}

.RealtorListing .row .realtorpic1 .img {
    width: 73%;
    height: 65px;
}

.RealtorListing .row .realtortext1 .textarea1 {
    width: 100%;
    height: 65px;
}

.RealtorListing .realtor-listing .about .p{
    padding-top: 25px;

}
.RealtorListing .row .realtortext1 .rev1 {
    float:right;

}

.RealtorListing .row .realtortext1 .cont1 {
    float:right;
    margin-left: 13px;

}


.RealtorListing h2 {
    padding-left: 30%;
    font-family: 'Raleway', sans-serif;
}

.RealtorListing .row .realtorpic1 .rate1 {
    padding-left: 32%;
}

/**** MEDIA QUERIES ****/
@media only screen and (max-width: 991px) {
    .RealtorListing .realtor-listing {
        padding: 5px!important;
    }
    .RealtorListing .realtor-listing a {
        margin: 0;
    }
    .RealtorListing .realtor-listing .media-body {
        padding: 10px;
    }
}

@media only screen and (min-width: 992px) {
    .RealtorListing .realtor-listing img {
        max-width: 180px;
    }
}


/**** BASE ****/
body {
    color: #888;
}
a {
    color: #03a1d1;
