@import url(http://fonts.googleapis.com/css?family=Raleway|Lora);

.navibar {
    position: absolute;
    background-color: #00bf8f;
    border-bottom-color: white;
    font-family: 'Raleway', sans-serif;
}

.navbar-brand {
    margin: 0 auto;
    vertical-align: middle;
}
.navbar-brand img {
    height: 30px;
    margin-top: -5px;
}

.navibar .navlist .list a {
    color: #fff;
    font-size: 18px;
}

.navibar .navlist .logList a {
    /*padding-top: 28px;*/
    color: #fff;
    font-size: 18px;
    padding-bottom: 2px;
}

.navibar .navlist .logList {
    padding-right: 12%;
}

.navibar .navlist .logList .dropdown .dropdown-menu {
    background-color: #00bf8f;
    border-color: white;
}

.navbar-toggle :hover {
    background-color: #00bf8f;
}

.navbar-default .navbar-toggle .icon-bar {
    background-color: #fff;
}

#classProject {
    margin: 0 auto;
    text-align: center;
    /*padding-right: 10%;*/
    position: absolute;
    width: 100%;
    color: #7B3F00;
    /*font-family: 'Lora', serif;*/
}

@media(max-width:768px) {
    #classProject {
        visibility: hidden;
    }
    .navbar-brand {
      position: absolute;
      width: 100%;
      margin: 0 auto;
      text-align: center;
    }
}
