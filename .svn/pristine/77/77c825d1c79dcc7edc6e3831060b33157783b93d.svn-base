@import url(http://fonts.googleapis.com/css?family=Raleway);

.navibar {
    background-color: #00bf8f;
    border-bottom-color: white;
    font-family: 'Raleway', sans-serif;
}

.navbar-brand {
    margin: 0 auto;
    vertical-align: middle;
    margin-top: -5px;
}
.navbar-brand img {
    height: 48px;
    width: 63px;
    
}

.navibar .navlist .list a, .navibar .navlist .logList a {
    color: #fff;
    font-size: 18px;
}

.navibar .navlist .list a:hover, .navibar .navlist .logList a:hover, .navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus {
    background-color: #fff;
    color: #00bf8f;
}

/*.navibar .navlist .logList {
    padding-right: 12%;
}*/

.navibar .navlist .logList .dropdown .dropdown-menu, .navbar-default .navbar-toggle :hover {
    background-color: #00bf8f;
    border-color: white;
}

.navbar-default .navbar-toggle .icon-bar {
    background-color: #fff;
}

#classProject {
    color: #7B3F00;
    padding-right: 30px;
}


@media(max-width: 768px) {
    #classProject {
        visibility: hidden;
    }
    .navbar-brand {
        position: absolute;
        /*width: 100%;    // for center logo in mobile*/
        /*margin: 0 auto; // but breaks collapse menu*/
        text-align: center;
    }
    
    .navbar-brand img {
    height: 40px;
    width: 63px;
    
}
}

@-moz-document url-prefix() { 
    .navbar-brand img {
    height: 40px;
    width: 63px;
    }
}