#modal {
    display: none;
    width: 400px;
    height: 68px;
    border:1px solid #CCC;
    background:white;
margin-left: 90px;

}

#modal:target {
    display: block;
    background:white;
    font:black;
    font-size: 18px;
}

#close {
    display: none;
}




#buy {
